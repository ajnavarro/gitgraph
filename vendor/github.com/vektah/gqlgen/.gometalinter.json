{
  "sort": ["path"],
  "Linters": {
    "errcheck": {
      "Command":           "errcheck -abspath -ignore '[rR]ead|[wW]rite'",
      "Pattern":           "PATH:LINE:COL:MESSAGE",
      "InstallFrom":       "github.com/kisielk/errcheck",
      "PartitionStrategy": "packages"
    }
  },
  "Disable": ["gas","golint","gocyclo","goconst", "gotype"]
}
